{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "preserve",
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "allowJs": true,
    "declaration": true,
  },
  "exclude": ["node_modules"]
}
