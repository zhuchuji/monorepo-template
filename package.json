{
  "name": "monorepo-template",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/**"
    ]
  },
  "scripts": {
    "dev": "start-storybook -p 6006",
    "build-doc": "build-storybook",
    "clean": "lerna exec -- yarn run clean",
    "build": "lerna exec -- yarn run build",
    "create": "ts-node ./scripts/create",
    "release": "ts-node scripts/release",
    "lint": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier"
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.25",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.18.9",
    "@storybook/addon-actions": "^6.4.0-alpha.27",
    "@storybook/addon-essentials": "^6.4.0-alpha.27",
    "@storybook/addon-links": "^6.4.0-alpha.27",
    "@storybook/react": "^6.4.0-alpha.27",
    "@types/postcss-url": "^8.0.2",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.2",
    "create-react-app": "^5.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "husky": "4.3.8",
    "lerna": "^4.0.0",
    "lint-staged": "10.5.3",
    "node-plop": "^0.26.3",
    "prettier": "^2.4.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.4.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
